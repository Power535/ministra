{
  "name": "magcore-app-browser",
  "version": "2.11.0",
  "description": "Browser application for Ministra TV Platform.",
  "author": {
    "name": "Infomir",
    "email": "support@infomir.eu"
  },
  "keywords": [
    "magcore",
    "application",
    "ministra",
    "browser"
