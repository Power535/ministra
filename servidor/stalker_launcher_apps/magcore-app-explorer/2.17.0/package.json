      "app": "/",
      "entry": "index.html",
      "icons": {
        "480": {
          "logoNormal": "img/480/logo.normal.png",
          "logoActive": "img/480/logo.active.png",
          "primeLogoNormal": "img/480/prime.logo.normal.png",
          "primeLogoActive": "img/480/prime.logo.active.png"
        },
        "576": {
          "logoNormal": "img/576/logo.normal.png",
          "logoActive": "img/576/logo.active.png",
          "primeLogoNormal": "img/576/prime.logo.normal.png",
          "primeLogoActive": "img/576/prime.logo.active.png"
        },
        "720": {
          "logoNormal": "img/720/logo.normal.png",
          "logoActive": "img/720/logo.active.png",
          "primeLogoNormal": "img/720/prime.logo.normal.png",
          "primeLogoActive": "img/720/prime.logo.active.png"
        },
        "1080": {
          "logoNormal": "img/1080/logo.normal.png",
          "logoActive": "img/1080/logo.active.png",
          "primeLogoNormal": "img/1080/prime.logo.normal.png",
          "primeLogoActive": "img/1080/prime.logo.active.png"
        }
      }
    },
    "colors": {
      "splashFont": "#fff",
      "splashBackground": "#5876cf"
    },
    "contentProviders": [
      "magcore-plugin-content-explorer"
    ],
    "actions": {
      "explorer:openResource": true
    }
  },
